    <!--Addition of Recycler Layout -->
    <android.support.v7.widget.RecyclerView
        android:id="@+id/listView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>


</android.support.constraint.ConstraintLayout>
